<Window x:Class="Space_invaders_Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Space_invaders_Game"
        mc:Ignorable="d"
        Title="Space invaders Game" Height="500" Width="800" 
        KeyDown="Window_KeyDown">
    <Canvas Name="myCanvas" Focusable="True" Background="Black" KeyDown="KeyIsDown" KeyUp="KeyIsUp" Margin="0,0,0,-6">

        <Label Name="enemiesLeft" Foreground="White" FontSize="16" FontWeight="ExtraBold" HorizontalAlignment="Left" VerticalAlignment="Top">Enemies left: 0</Label>

        <Label Name="Points" Foreground="White" FontSize="16" FontWeight="ExtraBold" HorizontalAlignment="Left" Canvas.Left="721" VerticalAlignment="Top">Points: 0</Label>
 

        <Rectangle Name="player" Fill="White" Height="55" Width="54" Canvas.Left="373" Canvas.Top="410" HorizontalAlignment="Center" VerticalAlignment="Top" />




    </Canvas>
</Window>
